		"@wangeditor/editor": "5.1.23",
		"@wangeditor/editor-for-vue": "5.1.12",
		"autoprefixer": "^10.4.7",
		"axios": "^1.3.3",
		"codemirror": "5.65.5",
		"crypto-js": "^3.1.9-1",
		"driver.js": "^0.9.8",
		"echarts": "^5.4.1",
		"element-china-area-data": "^6.1.0",
		"element-plus": "^2.8.1",
		"form-designer-plus": "^0.1.5",
		"js-cookie": "^3.0.1",
		"lodash": "^4.17.21",
		"mitt": "^3.0.0",
		"nprogress": "^0.2.0",
		"pinia": "^2.0.32",
		"qrcode": "1.5.1",
		"qs": "^6.11.0",
		"rollup-plugin-visualizer": "^5.12.0",
