		"vue-echarts": "6.6.1",
		"vue-i18n": "9.2.2",
		"vue-router": "^4.1.6",
		"vue3-pdf-app": "^1.0.3",
		"vue3-tree-org": "^4.2.2",
		"vue3-ts-jsoneditor": "2.9.0",
		"vue3-video-play": "1.3.1-beta.6",
